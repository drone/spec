kind: pipeline
spec:
  stages:
  - type: ci
    spec:
      steps:
      - type: script
        spec:
          image: golang
          run: |-
            go build
            go test
      - type: script
        spec:
          image: plugins/slack  
  - type: ci
    spec:
      steps:
      - type: script
        spec:
          image: node
          run: |-
            npm install
            npm run test
                
            